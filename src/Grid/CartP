Q[0] = x;
Q[1] = y;
Q[2] = z;
if (ao1xs){
	Qx[0] = 1;
	Qy[0] = 0;
	Qz[0] = 0;
	Qx[1] = 0;
	Qy[1] = 1;
	Qz[1] = 0;
	Qx[2] = 0;
	Qy[2] = 0;
	Qz[2] = 1;
	if (ao2ls || ao2xxs){
		Qxx[0] = 0;
		Qyy[0] = 0;
		Qzz[0] = 0;
		Qxx[1] = 0;
		Qyy[1] = 0;
		Qzz[1] = 0;
		Qxx[2] = 0;
		Qyy[2] = 0;
		Qzz[2] = 0;
		if (ao2xxs){
			Qxy[0] = 0;
			Qxz[0] = 0;
			Qyz[0] = 0;
			Qxy[1] = 0;
			Qxz[1] = 0;
			Qyz[1] = 0;
			Qxy[2] = 0;
			Qxz[2] = 0;
			Qyz[2] = 0;
		}
	}
}
